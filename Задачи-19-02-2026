SELECT ADDRESS
from STUDIO
where NAME = 'MGM';

SELECT MOVIESTAR.BIRTHDATE
from MOVIESTAR
where NAME = 'Sandra Bullock';

SELECT STARSIN.STARNAME
from STARSIN
where MOVIEYEAR = 1980
AND MOVIETITLE LIKE  '%Love%';

SELECT NAME
from MOVIEEXEC
where  NETWORTH > 10000000;

SELECT MOVIESTAR.NAME
from MOVIESTAR
where GENDER = 'M'
OR  ADDRESS LIKE '%Malibu%';

SELECT CLASS,CLASSES.COUNTRY
from CLASSES
where NUMGUNS <10;

SELECT NAME as SHIPNAME
from SHIPS
where LAUNCHED < 1918;

SELECT OUTCOMES.SHIP, BATTLE
from OUTCOMES
where result = 'sunk';

SELECT SHIPS.NAME
from SHIPS
where NAME = CLASS

SELECT SHIPS.NAME
from SHIPS
where NAME like 'R%';

SELECT SHIPS.NAME
from SHIPS
where NAME NOT LIKE  '% % %'
AND NAME LIKE '% %';

