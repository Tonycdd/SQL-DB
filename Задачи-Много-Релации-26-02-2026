SELECT *
FRom MOVIE;

SELECT *
    FROM STUDIO;

SELECT *
    FROM STARSIN;

SELECT *
    FROM MOVIESTAR;

SELECT MOVIE.STUDIONAME, TITLE, STARSIN.STARNAME
FROM MOVIE, STARSIN
WHERE TITLE = MOVIETITLE AND YEAR = MOVIEYEAR
AND LENGTH > 130
ORDER BY title;

SELECT NAME
FROM MOVIE , MOVIESTAR
WHERE GENDER = 'M' AND TITLE = 'Terms of Endearment';

SELECT MOVIESTAR.NAME
FROM MOVIE , MOVIESTAR
WHERE MOVIE.YEAR = 1995 AND MOVIE.STUDIONAME = 'MGM';

SELECT MOVIEEXEC.NAME
FROM MOVIEEXEC, MOVIE
WHERE MOVIE.STUDIONAME = 'MGM';

SELECT M2.TITLE
FROM MOVIE M1, MOVIE M2
WHERE M1.TITLE = 'Gone With the Wind'
AND M1.LENGTH > M2.LENGTH;

