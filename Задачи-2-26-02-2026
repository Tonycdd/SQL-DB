SELECT MODEL, PC.RAM as GB, PC.HD as MHz
FROM PC
WHERE PRICE < 1200;

SELECT DISTINCT MAKER
FROM PRODUCT
WHERE TYPE LIKE 'Printer';

SELECT MODEL, RAM, SCREEN
FROM LAPTOP
WHERE PRICE > 1000;

SELECT *
FROM PRINTER
WHERE COLOR LIKE 'y';

SELECT MODEL, SPEED, HD
FROM PC
WHERE (cd = '12x' OR cd ='16x')
AND PRICE < 2000;
